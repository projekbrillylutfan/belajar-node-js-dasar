{
  "name": "belajar-nodejs-restfullApi-studi-kasus",
  "version": "1.0.0",
  "description": "studi kasus nodejs restful api",
  "main": "src/main.js",
  "scripts": {
    "test": "jest -i",
    "dev": "node src/main.js"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!vendor/**/*.{js,jsx,ts,tsx}"
    ],
    "maxConcurrent": 1
  },
  "author": "billy uhuy",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@prisma/client": "^5.13.0",
    "bcrypt": "^5.1.1",
    "express": "^4.19.2",
    "joi": "^17.13.1",
    "uuid": "^9.0.1",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.5",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "prisma": "^5.13.0",
    "supertest": "^7.0.0"
  }
}
