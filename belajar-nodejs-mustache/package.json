{
  "name": "belajar-nodejs-mustache",
  "version": "1.0.0",
  "description": "belajar node js untuk mustache",
  "main": "./src/index.js",
  "type": "module",
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!vendor/**/*.{js,jsx,ts,tsx}"
      
    ],
    "maxConcurrent": 1
  },
  "author": "billy uhuy",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.4",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "@types/jest": "^28.1.0",
    "@types/mustache": "^4.2.0"
  },
  "dependencies": {
    "mustache": "^4.2.0"
  }
}
